<launch>
	<arg name="marker_size" default="4.5" />
	<arg name="max_new_marker_error" default="0.08" />
	<arg name="max_track_error" default="0.2" />

	<arg name="cam_image_topic" default="/kinect2/qhd/image_color" />
	<arg name="cam_info_topic" default="/kinect2/qhd/camera_info" />

	<arg name="output_frame" default="/base_footprint" />
	<arg name="bundle_files" default="
    $(find expe_ontologies)/experimentations/iros_paper/bundles/top_only/MarkerData_200_201_202_203_204_205.xml
    $(find expe_ontologies)/experimentations/iros_paper/bundles/top_only/MarkerData_206_207_208_209_210_211.xml
    $(find expe_ontologies)/experimentations/iros_paper/bundles/top_only/MarkerData_212_213_214_215_216_217.xml
    $(find expe_ontologies)/experimentations/iros_paper/bundles/top_only/MarkerData_218_219_220_221_222_223.xml
    $(find expe_ontologies)/experimentations/iros_paper/bundles/top_only/MarkerData_224_225_226_227_228_229.xml
    $(find expe_ontologies)/experimentations/iros_paper/bundles/top_only/MarkerData_230_231_232_233_234_235.xml
    $(find expe_ontologies)/experimentations/iros_paper/bundles/top_only/MarkerData_236_237_238_239_240_241.xml
    $(find expe_ontologies)/experimentations/iros_paper/bundles/top_only/MarkerData_242_243_244_245_246_247.xml
    $(find expe_ontologies)/experimentations/iros_paper/bundles/top_only/MarkerData_248_249_250_251_252_253.xml
    $(find expe_ontologies)/experimentations/iros_paper/bundles/top_only/MarkerData_254_255_256_257_258_259.xml
    $(find expe_ontologies)/experimentations/iros_paper/bundles/top_only/MarkerData_260_261_262_263_264_265.xml
    $(find expe_ontologies)/experimentations/iros_paper/bundles/top_only/MarkerData_266_267_268_269_270_271.xml" />

	<node name="ar_track_alvar" pkg="ar_track_alvar" type="findMarkerBundlesNoKinect" respawn="false" output="screen" args="$(arg marker_size) $(arg max_new_marker_error) $(arg max_track_error) $(arg cam_image_topic) $(arg cam_info_topic) $(arg output_frame) $(arg bundle_files)" />
</launch>
